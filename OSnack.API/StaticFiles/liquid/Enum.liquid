{% if HasDescription -%}
/** {{ Description }} */
{% endif -%}
export enum {{ Name }} {

{% for enumeration in Enums -%}
    {{ enumeration.Name }} = {{ enumeration.Value }},
{% endfor -%}
}

export const {{Name}}List=[
{% for enumeration in Enums -%} {Id:{{ enumeration.Value }},Name:"{{ enumeration.Name }}".replace(/([A-Z])/g, ' $1').trim(),Value:{{Name}}.{{enumeration.Name}}},
{% endfor -%}]